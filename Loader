local MarketplaceService = game:GetService("MarketplaceService")
local info = MarketplaceService:GetProductInfo(game.PlaceId)

if game.PlaceId == 13083893317 then
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Zyai-Lua/Inertia.mov/refs/heads/main/LifeSentence"))()
	wait()
	warn("Loaded! Enjoy.")

elseif game.PlaceId == 15558033314 or game.PlaceId == 16128274392 or game.PlaceId == 16110530195 or game.PlaceId == 108344270316113 then
